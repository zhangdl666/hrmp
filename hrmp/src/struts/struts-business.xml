        <action name="myUnreadMessage" class="com.platform.business.action.MessageAction" method="myUnreadMessage">
            <result name="success">/view/business/message/myUnreadMessage.jsp</result>
        </action>
        <action name="myReadMessage" class="com.platform.business.action.MessageAction" method="myReadMessage">
